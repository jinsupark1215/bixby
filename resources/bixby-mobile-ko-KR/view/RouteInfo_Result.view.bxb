// 결과 화면에 대한 보여주는 방식을 설정 (https://bixbydevelopers.com/dev/docs/reference/type/result-view)
result-view {
  // 결과 화면을 보여주고 싶은 output concept을 지정
  match: OptimalRouteResult(optimal)
  
  message ("알지 알지 그거 알지")
  
  render {
    // 어떻게 화면을 그릴 것인가에 대하여 설정
    // 결과 값 갯수에 따라 다르게 보여줌
    layout{
      layout-macro (RouteDetail) {
        param (optimal) {
          expression (optimal)
        }
      }
    }
  }
}