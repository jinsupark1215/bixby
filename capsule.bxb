capsule {
  id (playground.tellMeTransport)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-ko-KR)
  }
  runtime-flags {
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
    no-filtering-with-validation
  }
  capsule-imports {
    import(viv.geo) {
      as (geo)
      version (9.21.0)
    }
  }
  permissions {
    user-profile-access
  }
}
